<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Evaluation</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="" />
    <meta name="keywords" content="" />

    {% block linkApple %}
        <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('@UserBundle/Resources/robust-assets/ico/apple-icon-60.png')}}">
        <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('@UserBundle/Resources/robust-assets/ico/apple-icon-76.png')}}">
        <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('@UserBundle/Resources/robust-assets/ico/apple-icon-120.png')}}">
        <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('@UserBundle/Resources/robust-assets/ico/apple-icon-152.png')}}">
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('@UserBundle/Resources/robust-assets/ico/favicon.ico')}}">
        <link rel="shortcut icon" type="image/png" href="{{ asset('@UserBundle/Resources/robust-assets/ico/favicon-32.png')}}">
        <meta name="apple-mobile-web-app-capable" content="yes">
        <meta name="apple-touch-fullscreen" content="yes">
        <meta name="apple-mobile-web-app-status-bar-style" content="default">
    {% endblock %}

    {% block stylesheets %}
        {% stylesheets filter='cssrewrite'
        '@UserBundle/Resources/public/robust-assets/css/vendors.min.css'
        '@UserBundle/Resources/public/robust-assets/fonts/icomoon.css'
        '@UserBundle/Resources/public/robust-assets/fonts/flag-icon-css/css/flag-icon.min.css'
        '@UserBundle/Resources/public/robust-assets/css/plugins/sliders/slick/slick.css'
        '@UserBundle/Resources/public/robust-assets/css/plugins/forms/icheck/icheck.css'
        '@UserBundle/Resources/public/robust-assets/css/plugins/forms/icheck/custom.css'
        '@UserBundle/Resources/public/robust-assets/css/app.min.css'
        '@UserBundle/Resources/public/assets/css/style.css'

         %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}

    {% endblock %}
</head>
<body data-open="click" data-menu="vertical-compact-menu" data-col="1-column" class="vertical-layout vertical-compact-menu 1-column  blank-page blank-page">


    {% block loader %}
        <!-- START PRELOADER-->

        <div id="preloader-wrapper">
            <div id="loader">
                <div class="chasing-dots loader-black">
                    <div class="child dot1"></div>
                    <div class="child dot2"></div>
                </div>
            </div>
            <div class="loader-section section-top bg-amber"></div>
            <div class="loader-section section-bottom bg-amber"></div>
        </div>

        <!-- END PRELOADER-->

    {% endblock %}

    {% block header %}{% endblock %}

    <!-- container content - BEGIN -->

    {% block content %}{% endblock %}

    <!-- container content - END -->

    {% block footer %}{% endblock footer %}

{% block javascripts %}
    {% javascripts
    '@UserBundle/Resources/public/robust-assets/js/vendors.min.js'
    '@UserBundle/Resources/public/robust-assets/js/plugins/forms/icheck/icheck.min.js'
    '@UserBundle/Resources/public/robust-assets/js/plugins/forms/validation/jqBootstrapValidation.js'
    '@UserBundle/Resources/public/robust-assets/js/app.min.js'
    '@UserBundle/Resources/public/robust-assets/js/components/forms/form-login-register.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

</body>
</html>